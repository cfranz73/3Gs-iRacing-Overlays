{"Variables":{"DashboardVariables":[{"VariableName":"previousAllTimeBest","EvaluateOnlyOnce":false,"OverrideWithParentDashboardVariableWhenAvailable":true,"ValueExpression":{"JSExt":0,"Interpreter":1,"Expression":"// Remember this lap's best for displaying the delta on the next lap.\r\n// Because when we beat the best time, we don't have access to the previous best anymore.\r\nvar bestTime = $prop('PersistantTrackerPlugin.AllTimeBest');\r\nvar trackPercent = $prop('DataCorePlugin.GameData.TrackPositionPercent');\r\nif (trackPercent > 0.97)\r\n{\r\n    root['previousAllTimeBestTime'] = bestTime;\r\n}\r\n\r\nreturn root['previousAllTimeBestTime'];"},"EvaluateBeforeScreenRoles":false},{"VariableName":"previousSessionBestTime","EvaluateOnlyOnce":false,"OverrideWithParentDashboardVariableWhenAvailable":true,"ValueExpression":{"JSExt":1,"Interpreter":1,"Expression":"// Remember this lap's best for displaying the delta on the next lap.\r\n// Because when we beat the best time, we don't have access to the previous best anymore.\r\nvar bestTime = getSessionBestTime();\r\nvar trackPercent = $prop('DataCorePlugin.GameData.TrackPositionPercent');\r\nif (trackPercent > 0.97)\r\n{\r\n    root['previousSessionBestTime'] = bestTime;\r\n}\r\n\r\nreturn root['previousSessionBestTime'];"},"EvaluateBeforeScreenRoles":false},{"VariableName":"lapTimesUpdated","EvaluateOnlyOnce":false,"OverrideWithParentDashboardVariableWhenAvailable":true,"ValueExpression":{"JSExt":1,"Interpreter":1,"Expression":"// Returns true when both the trackPercent and currentLapTime have reset and \r\n// are both growing. Because trackPercent resets before currentLapTime is updated\r\n// when crossing the finish line.\r\nif (root['previousTrackPercent'] == null) { root['previousTrackPercent'] = 0; }\r\nif (root['previousTime'] == null) { root['previousTime'] = new Date('00:00:00.0'); }\r\n\r\nvar trackPercent = $prop('DataCorePlugin.GameData.TrackPositionPercent');\r\nvar time = new Date($prop('DataCorePlugin.Gam